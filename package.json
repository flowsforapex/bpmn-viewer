{
  "name": "bpmn-viewer",
  "version": "1.0.0",
  "description": "BPMN Viewer",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev-demo",
    "debug-webpack": "node --trace-deprecation node_modules/webpack/bin/webpack.js",
    "dev-demo": "webpack-dev-server --mode development --config webpack.config.demo.js",
    "dev-demo-prod": "webpack-dev-server --mode production --config webpack.config.demo.js",
    "build-all": "npm-run-all build-dist build-demo",
    "build-dist": "npm-run-all build-prod build-dev",
    "build-dev": "webpack --config webpack.config.dev.js",
    "build-prod": "webpack --config webpack.config.prod.js",
    "build-demo": "webpack --config webpack.config.demo.js",
    "update-packages": "npx remove-node-modules && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mt-ag/bpmn-viewer.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mt-ag/bpmn-viewer/issues"
  },
  "homepage": "https://github.com/mt-ag/bpmn-viewer#readme",
  "dependencies": {
    "bpmn-js": "^8.0.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^6.0.3",
    "npm-run-all": "^4.1.5",
    "webpack": "5.13.0",
    "webpack-cli": "4.3.1",
    "webpack-dev-server": "4.0.0-beta.0"
  }
}
